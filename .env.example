# Example environment for LogiMin visitor counter
# Copy this file to .env.local and fill in values. Do NOT commit .env.local.

# Upstash Redis REST (recommended primary provider)
UPSTASH_REDIS_REST_URL="https://<your-upstash-id>.upstash.io"
UPSTASH_REDIS_REST_TOKEN="<your-upstash-rest-token>"

# Counter identity (used by both providers)
VISITOR_NAMESPACE="your-namespace"   # e.g., logimin-surya or your domain
VISITOR_KEY="site-visits"            # e.g., prod-site-visits

# Optional Abacus fallback (only used if Upstash vars are missing)
ABACUS_NAMESPACE="your-namespace"
ABACUS_KEY="site-visits"

# Optional social URLs for footer buttons
NEXT_PUBLIC_GITHUB_URL="https://github.com/your-profile"
NEXT_PUBLIC_LINKEDIN_URL="https://www.linkedin.com/in/your-profile"
NEXT_PUBLIC_INSTAGRAM_URL="https://www.instagram.com/your-profile"
